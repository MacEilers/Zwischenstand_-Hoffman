public class Buchstabe {
    int Buchstabe_Ascii;
    String Buchstabe_String;
    int Anzahl;
    Buchstabe Links;
    Buchstabe Rechts;
    String Bin = "";
    
    // ggf. kann man aus der Q oder DyArray entnehem wie man auf folgeobjekt verweist  
    Buchstabe(int Buchstabe_A,String Buchstabe_S, int Anzahl){
        Buchstabe_Ascii = Buchstabe_A;
        Buchstabe_String = Buchstabe_S;
        this.Anzahl =Anzahl;
        
        
    }
    public Buchstabe getLinksBuchstabe() {
            return Links;
        }

        public void setLinksBuchstabe(Buchstabe Links) {
            this.Links = Links;
        }
        public Buchstabe getRechtsBuchstabe() {
            return Rechts;
        }

        public void setRechtsBuchstabe(Buchstabe Rechts) {
            this.Rechts = Rechts;
        }
        public String getBin() {
            return Bin;
        }

        public void setBin(String Bin) {
            this.Bin = Bin;
        }
        
        
}
